{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],

  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": ["error"],
    "no-unused-vars": "warn",
    "no-console": "off",
    "import/extensions": "off",
    "func-names": "off",
    "no-process-exit": "off",
    "semi": "off"
  }
}